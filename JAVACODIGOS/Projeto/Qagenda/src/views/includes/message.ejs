<% if (errors.length > 0) {%>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-danger">
                <% errors.forEach(error => {%>
                    <%= error %> <br>
                <% }) %>
            </div>
        </div>
    </div>
<% } %>
<% if (sucess.length > 0) {%>
    <div class="row">
        <div class="col my-3">
            <div lass="alert alert-success" style="background-color: rgba(0, 128, 0, 0.478); padding: 20px;" >
                <% sucess.forEach(s => {%>
                    <%= s %> <br>
                <% }) %>
            </div>
        </div>
    </div>
<% } %>